{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "default": {},
  "title": "Root Schema",
  "required": ["agentToken", "graphqlToken", "image","labels", "config"],
  "properties": {
    "agentToken": {
      "type": "string",
      "default": "",
      "minLength": 1,
      "title": "The agentToken Schema",
      "examples": [""]
    },
    "graphqlToken": {
      "type": "string",
      "default": "",
      "minLength": 1,
      "title": "The graphqlToken Schema",
      "examples": [""]
    },
    "image": {
      "type": "string",
      "default": "",
      "title": "The image Schema",
      "examples": ["ghcr.io/buildkite/agent-stack-k8s:latest"]
    },
    "nodeSelector": {
      "type": "object",
      "default": {},
      "title": "The nodeSelector Schema",
      "examples": [
        {
          "kubernetes.io/arch": "amd64"
        }
      ]
    },
    "labels": {
      "type": "object",
      "default": {},
      "title": "The labels Schema",
      "required": ["app"],
      "properties": {
        "app":{
          "type": ["string", "number"]
        },
        "user": {
          "type": "object",
          "default": {}
        },
        "project": {
          "type": "object",
          "default": {}
        }
      },
      "example": [
        "labels": {
          "app": "my-app1",
          "user": {
            "user": "name"
          },
          "project": {
            "project": "project"
          }
        }
      ]
    },
    "resources": {
      "type": "object",
      "default": {},
      "title": "The resources Schema",
      "properties": {
        "requests": {
          "type": "object",
          "default": {},
          "title": "The requests Schema",
          "properties": {
            "cpu": {
              "type": ["string", "number"]
            },
            "memory": {
              "type": ["string", "number"]
            }
          }
        },
        "limits": {
          "type": "object",
          "default": {},
          "title": "The limits Schema",
          "properties": {
            "cpu": {
              "type": ["string", "number"]
            },
            "memory": {
              "type": ["string", "number"]
            }
          }
        }
      },
      "examples": [
        {
          "requests": {
            "cpu": "100m",
            "memory": "100Mi"
          },
          "limits": {
            "memory": "100Mi"
          }
        }
      ]
    },
    "config": {
      "type": "object",
      "default": {},
      "title": "The config Schema",
      "required": ["org"],
      "properties": {
        "image": {
          "type": "string",
          "default": "",
          "title": "The image Schema",
          "examples": [""]
        },
        "debug": {
          "type": "boolean",
          "default": false,
          "title": "The debug Schema",
          "examples": [false]
        },
        "job-ttl": {
          "type": "string",
          "default": "",
          "title": "The job-ttl Schema",
          "examples": [""]
        },
        "max-in-flight": {
          "type": "integer",
          "default": 0,
          "title": "The max-in-flight Schema",
          "examples": [100]
        },
        "org": {
          "type": "string",
          "default": "",
          "minLength": 1,
          "title": "The org Schema",
          "examples": [""]
        },
        "tags": {
          "type": "array",
          "default": [],
          "title": "The tags Schema",
          "items": {
            "type": "string"
          },
          "examples": [["queue=kubernetes"]]
        },
        "cluster-uuid": {
          "type": "string",
          "default": "",
          "title": "The UUID of the Buildkite cluster to pull Jobs from",
          "examples": [""]
        }
      },
      "examples": [
        {
          "image": "",
          "debug": false,
          "job-ttl": "5m",
          "max-in-flight": 100,
          "org": "",
          "tags": []
        }
      ]
    }
  },
  "examples": [
    {
      "agentToken": "",
      "graphqlToken": "",
      "image": "ghcr.io/buildkite/agent-stack-k8s:latest",
      "nodeSelector": {},
      "labels": {
        "app": "",
        "user": {},
        "project": {}
      },
      "config": {
        "agentImage": "",
        "debug": false,
        "jobTTL": "",
        "maxInFlight": 100,
        "org": "",
        "tags": []
      }
    }
  ]
}
